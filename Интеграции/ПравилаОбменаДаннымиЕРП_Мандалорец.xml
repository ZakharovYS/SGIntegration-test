		<Алгоритм Имя="ДанныеКИ" ИспользуетсяПриЗагрузке="false">
			<Текст>ЗначенияПолей = СтрокаКИ.ЗначенияПолей;
	
Если ПустаяСтрока(ЗначенияПолей) И ЗначениеЗаполнено(СтрокаКИ.Представление) Тогда
	ЗначенияПолей = УправлениеКонтактнойИнформацией.КонтактнаяИнформацияПоПредставлению(СтрокаКИ.Представление,
		СтрокаКИ.Тип);
КонецЕсли;

Если ПустаяСтрока(ЗначенияПолей) Тогда
	ОбщегоНазначения.СообщитьПользователю(СтрШаблон("Пустой набор в значениях полей в %1 в %2", СтрокаКИ.Представление, Источник));
ИначеЕсли УправлениеКонтактнойИнформациейКлиентСервер.ЭтоКонтактнаяИнформацияВJSON(ЗначенияПолей) Тогда
	ДанныеАдреса = УправлениеКонтактнойИнформациейСлужебный.JSONВКонтактнуюИнформациюПоПолям(ЗначенияПолей, СтрокаКИ.Тип);
Иначе
	Если УправлениеКонтактнойИнформациейКлиентСервер.ЭтоКонтактнаяИнформацияВXML(ЗначенияПолей) Тогда
		РезультатыЧтения = Новый Структура;
		XDTOКонтактная = УправлениеКонтактнойИнформациейЛокализация.КонтактнаяИнформацияИзXML(ЗначенияПолей, СтрокаКИ.Тип, РезультатыЧтения);
		Если РезультатыЧтения.Свойство("ТекстОшибки") Тогда
			// Распознали с ошибками, сообщим при открытии.
			ОбщегоНазначения.СообщитьПользователю(СтрШаблон("Ошибка получения XDTO контактной информации %1 в %2", СтрокаКИ.Представление, Источник));
		КонецЕсли;
	Иначе
		XDTOКонтактная = УправлениеКонтактнойИнформациейЛокализация.АдресXMLВXDTO(ЗначенияПолей, СтрокаКИ.Представление);
		Если Параметры.Свойство("Страна") И ЗначениеЗаполнено(Параметры.Страна) Тогда
			Если ТипЗнч(Параметры.Страна) = ТипЗнч(Справочники.СтраныМира.ПустаяСсылка()) Тогда
				СтранаСсылка = Параметры.Страна; // СправочникСсылка.СтраныМира
				XDTOКонтактная.Состав.Страна = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(СтранаСсылка , "Наименование");
			Иначе
				XDTOКонтактная.Состав.Страна = Строка(Параметры.Страна);
			КонецЕсли;
		Иначе
			XDTOКонтактная.Состав.Страна = Справочники.СтраныМира.Россия.Наименование;
		КонецЕсли;
	КонецЕсли;
	
	ДанныеАдреса = УправлениеКонтактнойИнформациейСлужебный.КонтактнаяИнформацияВСтруктуруJSON(XDTOКонтактная, СтрокаКИ.Тип);
	НаселенныйПунктДетально = РаботаСАдресами.ПодготовитьАдресДляВвода(ДанныеАдреса);
КонецЕсли;

Если Не ДанныеАдреса = Неопределено Тогда
	ДанныеКИ.Комментарий = ДанныеАдреса.comment;
	ДанныеКИ.Поле1       = ДанныеАдреса.ZIPCode;
	ДанныеКИ.Поле2       = ДанныеАдреса.area + " " + ДанныеАдреса.areatype;         // регион
	ДанныеКИ.Поле3       = ДанныеАдреса.district + " " + ДанныеАдреса.districtType;   // район области
	ДанныеКИ.Поле4       = ДанныеАдреса.city + " " + ДанныеАдреса.cityType;         // город
	ДанныеКИ.Поле5       = ДанныеАдреса.locality + " " + ДанныеАдреса.localityType; // нас. пункт
	ДанныеКИ.Поле6       = ДанныеАдреса.street + " " + ДанныеАдреса.streetType;     // улица
	ДанныеКИ.Поле7       = ДанныеАдреса.houseNumber;                                // дом
	
	Если ТипЗнч(ДанныеАдреса.buildings) = Тип("Массив") 
		И ДанныеАдреса.buildings.Количество() Тогда
		ДанныеКИ.Поле8   = ДанныеАдреса.buildings[0].number; // литера
	Иначе
		ДанныеКИ.Поле8   = ""; // литера
	КонецЕсли;
	
	Если ТипЗнч(ДанныеАдреса.apartments) = Тип("Массив") 
		И ДанныеАдреса.apartments.Количество() Тогда
		ДанныеКИ.Поле9   = ДанныеАдреса.apartments[0].number; // квартира
	Иначе
		ДанныеКИ.Поле9   = ""; // квартира
	КонецЕсли;
	ДанныеКИ.Поле10      = "";
КонецЕсли;</Текст>
