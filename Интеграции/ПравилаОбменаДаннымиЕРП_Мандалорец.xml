		<Алгоритм Имя="УстановитьНеобходимостьВыгрузкиЗаменыДокумента" ИспользуетсяПриЗагрузке="false">
			<Текст>
Если ТипЗнч(ВходящиеДанные) &lt;&gt; Тип("Структура") Тогда
	ВходящиеДанные = Новый Структура;
КонецЕсли;


Если ТипЗнч(Источник) &lt;&gt; Тип("Структура") Тогда
	
	Если ВходящиеДанные.Свойство("ТолькоПолучитьУзелСсылки") Тогда
		ТолькоПолучитьУзелСсылки = ВходящиеДанные.ТолькоПолучитьУзелСсылки;
	КонецЕсли;
	
	
	Если ВходящиеДанные.Свойство("НеЗамещатьОбъектПриЗагрузке") И ВходящиеДанные.НеЗамещатьОбъектПриЗагрузке = Истина Тогда
		
		Если Параметры.ВыгруженныеДокументы.Найти(Источник.Ссылка) = Неопределено Тогда
			
			НеЗамещатьОбъектПриЗагрузке = Истина;
			ЗапоминатьВыгруженные = Ложь;
			Если Параметры.ВыгруженныеОбъектыБезЗамены.Найти(Источник.Ссылка) &lt;&gt; Неопределено Тогда
				ТолькоПолучитьУзелСсылки = Истина;
			КонецЕсли;
			
			Параметры.ВыгруженныеОбъектыБезЗамены.Добавить(Источник.Ссылка);
			
		КонецЕсли;
		
	Иначе
		
		Если Не ТолькоПолучитьУзелСсылки Тогда
			Параметры.ВыгруженныеДокументы.Добавить(Источник.Ссылка);
		КонецЕсли;
		
	КонецЕсли;
	
КонецЕсли;</Текст>
			<Параметры>Источник, ВходящиеДанные</Параметры>
		</Алгоритм>
		<Алгоритм Имя="УстановитьПометкуУдаленияИзВходящихДанных" ИспользуетсяПриЗагрузке="false">
			<Текст>
Если ТипЗнч(ВходящиеДанные) = Тип("Структура")
	И ВходящиеДанные.Свойство("ПометкаУдаления") Тогда
	
	Значение = ВходящиеДанные.ПометкаУдаления;
	
КонецЕсли;</Текст>
			<Параметры>ВходящиеДанные</Параметры>
		</Алгоритм>
		<Алгоритм Имя="УстановитьПроведениеИзВходящихДанных" ИспользуетсяПриЗагрузке="false">
			<Текст>
Если ТипЗнч(ВходящиеДанные) = Тип("Структура")
	И ВходящиеДанные.Свойство("Проведен") Тогда
	
	Значение = ВходящиеДанные.Проведен;
	
КонецЕсли;</Текст>
			<Параметры>ВходящиеДанные</Параметры>
		</Алгоритм>
		<Алгоритм Имя="ЭтоДарко" ИспользуетсяПриЗагрузке="true">
			<Текст>Попытка
//	Объект.ЭтоДарко = Истина;
Исключение
КонецПопытки;</Текст>
		</Алгоритм>
	</Алгоритмы>
	<Запросы>
		<Группа Имя="ВозвратТоваровПоставщику">
			<Запрос Имя="ДроблениеВозвратТоваровПоставщику" ИспользуетсяПриЗагрузке="false">
				<Текст>
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ВозвратТоваровПоставщикуТовары.Ссылка КАК Документ,
	ВозвратТоваровПоставщикуТовары.Номенклатура.ГруппаФинансовогоУчета КАК ГруппаФинансовогоУчета
ПОМЕСТИТЬ Товары
ИЗ
	Документ.ВозвратТоваровПоставщику.Товары КАК ВозвратТоваровПоставщикуТовары
ГДЕ
	ВозвратТоваровПоставщикуТовары.Ссылка = &amp;Ссылка
	И ВозвратТоваровПоставщикуТовары.Ссылка.Проведен
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	кс_ДроблениеОбъектовПриИнтеграции.ОбъектИсточник КАК Документ,
